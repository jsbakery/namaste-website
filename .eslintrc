{
  "extends": "airbnb",
  "rules": {
    "indent": ["error", 4],
    "react/jsx-indent": ["error", 4],
    "react/jsx-indent-props": ["error", 4],
    "react/forbid-prop-types": ["off"],
    "react/jsx-props-no-spreading": ["off"],
    "import/prefer-default-export": ["off"]
  },
  "env": {
    "browser": true
  },
  "globals": {
    "fetch": true
  },
  "parserOptions": {
    "ecmaVersion": 2020
  }
}
